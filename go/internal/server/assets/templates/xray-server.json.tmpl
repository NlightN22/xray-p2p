{
  "log": {
    "loglevel": "warning",
    "access": "none"
  },
  "inbounds": [
    {
      "tag": "xp2p-trojan",
      "listen": "0.0.0.0",
      "port": {{.Port}},
      "protocol": "trojan",
      "settings": {
        "clients": []
      },
      "streamSettings": {
        "network": "tcp",
{{- if .TLS }}
        "security": "tls",
        "tlsSettings": {
          "certificates": [
            {
              "certificateFile": "{{.CertificateFile}}",
              "keyFile": "{{.KeyFile}}"
            }
          ]
        }
{{- else }}
        "security": "none"
{{- end }}
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct"
    }
  ]
}
